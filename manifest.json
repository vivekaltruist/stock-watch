{
  "manifest_version": 2,

  "name": "Stock watch",
  "description": "This extension is used to add and display your favourite stocks price",
  "version": "1.0",

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html",
    "default_title": "Stocks"
  },
  "permissions": [
    "storage",
    "activeTab",
    "https://ajax.googleapis.com/",
    "https://www.google.com/*"
  ]
}
